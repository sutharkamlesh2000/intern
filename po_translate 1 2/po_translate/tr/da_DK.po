#. module: employee_management
#: model:mail.template,body_html:employee_management.school_mail_template
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p t-field=\"object.name\">Dear <br> In your school, you have these courses. </p>\n"
"                <table class=\"table table-striped\">\n"
"                    <thead>\n"
"                        <tr>\n"
"                            <th class=\"text-center\">Sr no</th>\n"
"                            <th class=\"text-center\">Course</th>\n"
"                        </tr>\n"
"                    </thead>\n"
"                    <tbody>\n"
"                        <t t-if=\"object.courses_ids\">\n"
"                            <t t-set=\"i\" t-value=\"1\"></t>\n"
"                            <t t-foreach=\"object.courses_ids\" t-as=\"course\">\n"
"                                <tr>\n"
"                                    <td class=\"text-center\" scope=\"row\">\n"
"                                        <span t-esc=\"i\"></span>\n"
"                                        <t t-set=\"i\" t-value=\"i+1\"></t>\n"
"                                    </td>\n"
"                                    <td class=\"text-center\" scope=\"row\">\n"
"                                        <span t-esc=\"course.course_name\"></span>\n"
"                                    </td>\n"
"                                </tr>\n"
"                            </t>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <tr>\n"
"                                <td class=\"text-center\" scope=\"row\">\n"
"                                    No courses found\n"
"                                </td>\n"
"                            </tr>\n"
"                        </t>\n"
"                    </tbody>\n"
"                </table> \n"
"                <br>\n"
"                Congratulations on your school registration! Thankyou </div>\n"
"            <div class=\"\">\n"
"                <p> Test condition statement <p t-if=\"object.name == 'Mayur Public School'\">\n"
"                        True\n"
"                    </p>\n"
"                    <p t-else=\"\">\n"
"                        False\n"
"                    </p>\n"
"                </p>\n"
"            </div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
" <p t-field=\"object.name\">Kære <br> På din skole har du disse kurser. </p>\n"
" <table class=\"table table-striped\">\n"
" <thead>\n"
" <tr>\n"
" <th class=\"text-center\">Sr no</th>\\ n <th class=\"text-center\">Kursus</th>\n"
" </tr>\n"
" </thead>\n"
" <tbody>\n"
" <t t-if=\"object.courses_ids\" >\n"
" <t t-set=\"i\" t-value=\"1\"></t>\n"
" <t t-foreach=\"object.courses_ids\" t-as=\"kursus \">\n"
" <tr>\n"
" <td class=\"text-center\" scope=\"row\">\n"
" <span t-esc=\"i\"></span>\n"
" < t t-set=\"i\" t-value=\"i+1\"></t>\n"
" </td>\n"
" <td class=\"text-center\" scope=\"row \">\n"
" <span t-esc=\"course.course_name\"></span>\n"
" </td>\n"
" </tr>\n"
" </t>\n"
" </t>\n"
" < t t-else=\"\">\n"
" <tr>\n"
" <td class=\"text-center\" scope=\"row\">\n"
" Ingen kurser fundet\n"
" </td>\n"
" < /tr>\n"
" </t>\n"
" </tbody>\n"
" </table> \n"
" <br>\n"
" Tillykke med din skoleregistrering! Tak </div>\n"
" <div class=\"\">\n"
" <p> Testtilstandserklæring <p t-if=\"object.name == 'Mayur Public School'\">\n"
" Sandt\n"
" </p>\n"
" <p t-else=\"\">\n"
" Falsk\n"
" </p>\n"
" </p>\n"
" </div>\n"

